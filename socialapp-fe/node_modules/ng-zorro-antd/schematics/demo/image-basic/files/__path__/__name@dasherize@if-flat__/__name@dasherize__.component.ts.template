  selector: '<%= selector %>',
  standalone: true,
  imports: [NzImageModule],
  <% if(inlineTemplate) { %>template: `
    <img
      nz-image
      width="200px"
      height="200px"
      nzSrc="https://zos.alipayobjects.com/rmsportal/jkjgkEfvpUPVyRjUImniVslZfWPnJuuZ.png"
      alt=""
    />
  `<% } else { %>templateUrl: './<%= dasherize(name) %>.component.html'<% } %>
})
